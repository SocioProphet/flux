{
  "name": "junto",
  "version": "0.2.4",
  "private": true,
  "description": "Junto ad4m chat application leveraging distributed & decentralized technologies",
  "author": "josh@junto.foundation",
  "repository": {
    "type": "git",
    "url": "https://github.com/juntofoundation/communities.git"
  },
  "scripts": {
    "lint": "vue-cli-service lint",
    "get-languages": "node scripts/get-languages.js",
    "build-start": "cd ../ad4m-core-executor && npm run build && cd ../communities && npm run electron:serve",
    "clean-state": "node scripts/clean-state.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "build-holochain": "echo \"\n\n\n\nThis next stage will fail if Nix is not installed!! You can install here: https://nixos.wiki/wiki/Nix_Installation_Guide\" && sleep 5 && nix-shell --run ./scripts/copy-hc.sh",
    "release": "npm run get-languages && npm run electron:build",
    "test": "jest --env=jsdom",
    "test-gql": "jest --env=jsdom src/__tests__/graphql.ts"
  },
  "main": "background.js",
